{
  "name": "gwt-react",
  "version": "1.0.0",
  "main": "lib/gwt-react-bundle.js",
  "scripts": {
    "clean": "./node_modules/.bin/rimraf lib node_modules",
    "build": "webpack -d index=./src/index-react.js --output-filename=./lib/gwt-react-bundle.js",
    "build:min": "webpack -p index=./src/index-react.js --output-filename=./lib/gwt-react-bundle.min.js",
    "build-react-router": "webpack -d index=./src/index-react-router.js --output-filename=./lib/gwt-react-router-bundle.js",
    "build-react-router:min": "webpack -p index=./src/index-react-router.js --output-filename=./lib/gwt-react-router-bundle.min.js",
    "build-react-component-interop": "webpack -d index=./src/index-react-component-interop.js --output-filename=./lib/gwt-react-component-interop-bundle.js",
    "build-react-component-interop:min": "webpack -p index=./src/index-react-component-interop.js --output-filename=./lib/gwt-react-component-interop-bundle.min.js",
    "build-react-blueprint": "webpack -d index=./src/index-react-blueprint.js --output-filename=./lib/gwt-react-blueprint-bundle.js",
    "build-react-blueprint:min": "webpack -p index=./src/index-react-blueprint.js --output-filename=./lib/gwt-react-blueprint-bundle.min.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-core": "^6.26.3",
    "compression-webpack-plugin": "2.0.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "css-loader": "^2.1.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0"
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "fine-uploader": "^5.16.2",
    "fine-uploader-wrappers": "^1.0.1",
    "react-fine-uploader": "^1.1.1",
    "react-transition-group": "^2.5.3",
    "prop-types": "^15.6.2",
    "react-router": "^4.3.1",
    "@blueprintjs/core": "^3.11.0",
    "@blueprintjs/select": "^3.5.0",
    "@blueprintjs/table": "^3.4.0"
  }
}
